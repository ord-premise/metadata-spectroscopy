{
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "$id": "https://example.com/dosing.schema.json",
    "title": "Dosing",
    "description": "Dosing",
    "type": "object",
    "properties": {
        "name": {
            "type": "string",
            "description": "The name of the dosing."
        },
        "moleculeName": {
            "type": "string",
            "description": "The name of the used gas molecule."
        },
        "duration": {
            "type": "object",
            "description": "Duration of the dosing.",
            "properties": {
                "value": {
                    "type": "number"
                },
                "unit": {
                    "type": "string"
                }
            }
        },
        "pressure": {
            "type": "object",
            "description": "Pressure of the dosing.",
            "properties": {
                "value": {
                    "type": "number"
                },
                "unit": {
                    "type": "string"
                }
            }
        },
        "comments": {
            "type": "string",
            "description": "Comments to the deposition."
        },
        "required": ["name", "moleculeName", "duration"]
    }
}