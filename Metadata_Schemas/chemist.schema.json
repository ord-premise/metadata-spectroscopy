{
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "$id": "https://example.com/chemist.schema.json",
    "title": "Chemist",
    "description": "Chemist",
    "type": "object",
    "properties": {
        "name": {
            "type": "string",
            "description": "The name of the supplier."
        },
        "email": {
            "type": "string",
            "description": "The email of the supplier."
        },
        "workPhone": {
            "type": "string",
            "description": "The work phone of the supplier."
        },
        "supplier": {
            "$ref": "./supplier.schema.json"
        },
        "required": ["name", "address", "supplier"]
    }
}